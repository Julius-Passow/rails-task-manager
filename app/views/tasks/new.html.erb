<%= form_for @task do |f| %>
  <%= f.label :title%>
  <%= f.text_field :title%>

  <%= f.label :description%>
  <%= f.text_field :description%>

  <%= f.label :completed%>
  <%= f.check_box :completed%>
  <%= f.submit %>
<% end%>
